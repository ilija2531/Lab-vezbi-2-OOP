package zadaca4;

public class Main {

	public static void main(String[] args) {
		Avtomobil avtomobil1 = new Avtomobil();
		avtomobil1.marka = " Renault ";
		avtomobil1.model = " Megane ";
		avtomobil1.pomkm = 230000;
		
		System.out.println("Podatoci za prviot avtomobil: ");
		System.out.println("Marka: " + avtomobil1.marka + "Model: " + avtomobil1.model + "Pominati kilometri: " + avtomobil1.pomkm);
		
		Avtomobil avtomobil2 = new Avtomobil("Toyota ", "Yaris ", 170000);
		System.out.println("Podatoci za vtoriot avtomobil: ");
		System.out.println("Marka: " + avtomobil2.marka + "Model: " + avtomobil2.model + "Pominati kilometri: " + avtomobil2.pomkm);
		
		

	}

}
